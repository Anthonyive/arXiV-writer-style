@article{FishburnHammerBipartite1996,
title = "Bipartite dimensions and bipartite degrees of graphs",
journal = "Discrete Mathematics",
volume = "160",
number = "1",
pages = "127 - 148",
year = "1996",
issn = "0012-365X",
doi = "https://doi.org/10.1016/0012-365X(95)00154-O",
url = "http://www.sciencedirect.com/science/article/pii/0012365X9500154O",
author = "Peter C. Fishburn and Peter L. Hammer",
abstract = "A cover (bipartite) of a graph G is a family of complete bipartite subgraphs of G whose edges cover G's edges. G'sbipartite dimension d(G) is the minimum cardinality of a cover, and its bipartite degree η(G) is the minimum over all covers of the maximum number of covering members incident to a vertex. We prove that d(G) equals the Boolean interval dimension of the irreflexive complement of G, identify the 21 minimal forbidden induced subgraphs for d ⩽ 2, and investigate the forbidden graphs for d ⩽ n that have the fewest vertices. We note that for complete graphs, d(Kn) = [log2n], η(Kn) = d(Kn) for n ⩽ 16, and η(Kn) is unbounded. The list of minimal forbidden induced subgraphs for η ⩽ 2 is infinite. We identify two infinite families in this list along with all members that have fewer than seven vertices."
}

@article{GregoryBiclique1991,
title = "Biclique coverings of regular bigraphs and minimum semiring ranks of regular matrices",
journal = "Journal of Combinatorial Theory, Series B",
volume = "51",
number = "1",
pages = "73 - 89",
year = "1991",
issn = "0095-8956",
doi = "https://doi.org/10.1016/0095-8956(91)90006-6",
url = "http://www.sciencedirect.com/science/article/pii/0095895691900066",
author = "David A Gregory and Norman J Pullman and Kathryn F Jones and J.Richard Lundgren",
abstract = "We study the minimum number of complete bipartite subgraphs needed to cover and partition the edges of a k-regular bigraph on 2n vertices. Bounds are determined on the minima of these numbers for fixed n and k. Exact values of the minima are found for all n and k ≤ 4. The same results hold for directed graphs. Equivalently, we have determined bounds on the minimum value of the Boolean and nonnegative integer ranks of binary n × n matrices with constant row and column sum k for fixed n and k, obtaining the exact values of the minimum for k ≤ 4."
}

@article{TheoryOfAtomataBrzTamm2014,
title = "Theory of átomata",
journal = "Theoretical Computer Science",
volume = "539",
pages = "13 - 27",
year = "2014",
issn = "0304-3975",
doi = "https://doi.org/10.1016/j.tcs.2014.04.016",
url = "http://www.sciencedirect.com/science/article/pii/S0304397514002953",
author = "Janusz Brzozowski and Hellis Tamm",
keywords = "Atom, Atomic NFA, Átomaton, Left quotient, Minimization by double reversal, NFA, Regular language",
abstract = "We show that every regular language defines a unique nondeterministic finite automaton (NFA), which we call “átomaton”, whose states are the “atoms” of the language, that is, non-empty intersections of complemented or uncomplemented left quotients of the language. We describe methods of constructing the átomaton, and prove that it is isomorphic to the reverse automaton of the minimal deterministic finite automaton (DFA) of the reverse language. We study “atomic” NFAs in which the right language of every state is a union of atoms. We generalize Brzozowski's double-reversal method for minimizing a deterministic finite automaton (DFA), showing that the result of applying the subset construction to an NFA is a minimal DFA if and only if the reverse of the NFA is atomic. We prove that Sengoku's claim that his method always finds a minimal NFA is false."
}

