@Article{Dai-Wang,
	author = "Dai, Xianzhe
	and Wang, Changliang",
	title = "Perelman's $\lambda$-Functional on Manifolds with Conical Singularities",
	journal = "The Journal of Geometric Analysis",
	year = "2018",
	month = "Dec",
	day = "01",
	volume = "28",
	number = "4",
	pages = "3657--3689",
	abstract = "In this paper, we prove that on a compact manifold with isolated conical singularity, the spectrum of the Schr{\"o}dinger operator {\$}{\$}-4{\backslash}Delta +R{\$}{\$}-4$\Delta$+Rconsists of discrete eigenvalues with finite multiplicities, if the scalar curvature R satisfies a certain condition near the singularity. Moreover, we obtain an asymptotic behavior for eigenfunctions near the singularity. As a consequence of these spectral properties, we extend the theory of the Perelman's {\$}{\$}{\backslash}lambda {\$}{\$}$\lambda$-functional on smooth compact manifolds to compact manifolds with isolated conical singularities.",
	issn = "1559-002X",
	doi = "10.1007/s12220-017-9971-4",
	url = "https://doi.org/10.1007/s12220-017-9971-4"
}

@Article{ozu0,
	author="Ozuch, Tristan",
	title="Perelman's Functionals on Cones",
	journal="The Journal of Geometric Analysis",
	year="2019",
	month="Jan",
	day="03",
	abstract="In this paper, we are interested in conical structures of manifolds with respect to the Ricci flow. In a first part, we study Perelman's {\$}{\$}{\backslash}lambda {\$}{\$}$\lambda$and {\$}{\$}{\backslash}nu {\$}{\$}$\nu$functionals of cones and characterize their finiteness in terms of the {\$}{\$}{\backslash}lambda {\$}{\$}$\lambda$-functional of the link. As an application, we characterize manifolds with conical singularities on which a {\$}{\$}{\backslash}lambda {\$}{\$}$\lambda$-functional can be defined and get upper bounds on the {\$}{\$}{\backslash}nu {\$}{\$}$\nu$-functional of asymptotically conical manifolds. We then present an adaptation of the proof of Perelman's pseudolocality theorem and prove that cones over some perturbations of the unit sphere can be smoothed out by type III immortal solutions of the Ricci flow.",
	issn="1559-002X",
	doi="10.1007/s12220-018-00131-w",
	url="https://doi.org/10.1007/s12220-018-00131-w"
}

