    "import pandas as pd\n",
    "\n",
    "summary=pd.DataFrame({'Command':[]})\n",
    "\n",
    "#loop for each file here\n",
    "data = pd.read_csv(\"SampleCSV.csv\",usecols=['Command','Notes']) \n",
    "\n",
    "#find author&title\n",
    "author=[] \n",
    "title=''\n",
    "for i in range(0,len(data)):\n",
    "    if data['Command'][i] == '\\\\author':\n",
    "        author.append(data['Notes'][i])\n",
    "    if data['Command'][i] == '\\\\title':\n",
    "        title = data['Notes'][i]     \n",
    "\n",
    "summary[title]=[]\n",
    "\n",
    "#count commands\n",
    "counting={}\n",
    "List=data['Command']\n",
    "for i in List:\n",
    "    if i!='\\\\author' and i!='\\\\title':\n",
    "        if i in counting:\n",
    "            counting[i]+=1\n",
    "        else:\n",
    "            counting[i]=1\n",
    "\n",
    "{k: v for k, v in sorted(counting.items(), key=lambda item: item[1])}\n",
    "#for c in counting.keys():\n",
    "   # if c not in summary['Command']:\n",
    "      # summary.loc[len(summary)]=[c]+[]+[counting[c]]"
